//Test merging ShaderA into Base SPX bytecode
//ShaderA main function refers to type declared by both ShaderA and Base

shader Base
{
	int Var0;
	int Var1;
	
	const int BaseConst = 2;
	
	stream float sbase1;
	stream int sbase2;
};

shader shaderA : Base
{
	int Var0;
	stream int sa1;
	
	const int aConst = 5;
	
    cbuffer C1
    {
		int Var1;
    }
	
	cbuffer C2
    {
		int Var2;
    }
	
	int main()
	{
		return BaseConst + streams.sbase1 + Var1 + base.Var1 + aConst + streams.sa1;
	}
};
