shader ShaderMain
{
	rgroup
	{
		SamplerState Sampler0;
	}
	
	
	Texture2D Texture0;
	TextureCube CubeMap;

	void VSMain()
	{
		float2 uv2 = float2(0.5, 0.5);
		float4 color = Texture0.Sample(Sampler0, uv2);
	}
	
	void PSMain()
	{
		float3 direction = float3(1, 0, 0);
		float4 color = CubeMap.Sample(Sampler0, direction);
	}
}
