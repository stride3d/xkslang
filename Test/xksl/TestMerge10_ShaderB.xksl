//Similar to previous merge samples, except that here both ShaderA and ShaderB inherits from Base. Thanks to mixin, expected result is the same: 6

shader Base
{
	stream int ColorTarget: SV_Target0;

    int Compute()
    {
        return 1;
    }
    void main()
    {
        streams.ColorTarget = Compute();
    }
}

shader ShaderA : Base
{
    override int Compute()
    {
        return base.Compute() + 2;
    }
}

shader ShaderB : Base
{
    override int Compute()
    {
        return base.Compute() + 3;
    }
}