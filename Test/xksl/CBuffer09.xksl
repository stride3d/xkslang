shader ShaderComp
{
	stage cbuffer PerDraw
	{
		float varDraw1;
		float varDraw2;
	}
	
	cbuffer PerDraw
	{
		float varDraw3;
	}
	
	float Compute()
	{
		return varDraw1 + varDraw3;
	}
}

shader ShaderA
{
	compose ShaderComp colorA;
	compose ShaderComp colorB;
	
	float VSmain()
	{
		return colorA.Compute();
	}
	
	float PSmain()
	{
		return colorB.Compute();
	}
}

