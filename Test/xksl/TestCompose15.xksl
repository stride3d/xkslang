shader Utils
{
	static int Negate(int i)
	{
		return -i;
	}
	
	static int Add(int i, int k)
	{
		return i + k;
	}
}

shader CompA
{
	int Compute(int i)
	{
		return 0;
	}
}

shader CompB : CompA
{
	override int Compute(int i)
	{
		return Utils.Negate(i);
	}
}

shader ShaderA
{
	compose CompA comp;
	
	int main()
	{
		return comp.Compute(1);
	}
}

shader ShaderB : ShaderA
{
	override int main()
	{
		int i = base.main();
		return comp.Compute(2) + Utils.Add(i, 5);
	}
}