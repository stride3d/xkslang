shader ShaderComp
{
	float varC1;
	float varC2;
	
	cbuffer PerDraw
	{
		float varDraw1;
	}
	
	cbuffer unusedCB
	{
		float varUnused;
	}
	
	float Compute()
	{
		return varC1 + varDraw1;
	}
}

shader ShaderA
{
	float varA;
	
	compose ShaderComp colorA;
	compose ShaderComp colorB;
	
	float main()
	{
		return varA + colorA.Compute() + colorB.Compute();
	}
}
