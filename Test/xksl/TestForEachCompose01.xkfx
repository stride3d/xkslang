convertAndLoad "TestForEachCompose01.xksl"

mixer CompA
CompA.mixin( ComputeColorA )

mixer CompB
CompB.mixin( ComputeColorB )

mixer main
main.mixin( ShaderA )

main.addComposition( ShaderA.colors = CompA )
main.addComposition( ShaderA.colors = CompB )

main.setStageEntryPoint Pixel "main"
main.compile
