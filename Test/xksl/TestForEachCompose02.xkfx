convertAndLoad "TestForEachCompose02.xksl"

mixer CompA
CompA.mixin ComputeColorA

mixer CompB
CompB.mixin ComputeColorB

mixer main
main.mixin ShaderA

main.addComposition ShaderA.colorsA CompA
main.addComposition ShaderA.colorsA CompB
main.addComposition ShaderA.colorsB CompA
main.addComposition ShaderA.colorsB CompB

main.setStageEntryPoint Pixel "main"
main.compile