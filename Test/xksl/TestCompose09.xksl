//Compile a composition of a class having a composition too

shader SubComp
{
	int var1;
	stage stream int s2: S2;
	
	int Compute()
	{
		return var1 + streams.s2;
	}
}

shader Comp
{
	int var1;
	stage stream int s1: S1;
	
	compose SubComp sb;
	
	int Compute()
	{
		return var1 + streams.s1 + sb.Compute();
	}
}

shader ShaderA
{
	compose Comp color;
	compose SubComp sb;
	
	void main()
	{
		int i = color.Compute() + sb.Compute();
	}
}
