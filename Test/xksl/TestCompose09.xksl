//Compile a composition of a class having a composition too

shader SubComp
{
	int var1;
	stream int s1;
	
	int Compute()
	{
		return var1 + streams.s1;
	}
}

shader Comp
{
	int var1;
	stream int s1;
	
	compose SubComp sb;
	
	int Compute()
	{
		return var1 + streams.s1 + sb.Compute();
	}
}

shader ShaderA
{
	compose Comp color;
	compose SubComp sb;
	
	int main()
	{
		return color.Compute() + sb.Compute();
	}
}
