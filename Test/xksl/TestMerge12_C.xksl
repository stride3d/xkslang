shader Base
{
	stream int ColorTarget: SV_Target0;

    int Compute()
    {
        return 1;
    }
    void main()
    {
        streams.ColorTarget = Compute();
    }
}

shader A1 : Base
{
    override int Compute()
    {
        return base.Compute() + 2;
    }
}

shader A2 : Base
{
    override int Compute()
    {
        return base.Compute() + 20;
    }
}

shader C : A1, A2
{
    override int Compute()
    {
        return base.Compute() + 100;
    }
}
