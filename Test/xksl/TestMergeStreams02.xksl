
shader ShaderA
{
	stage stream float4 outStreamA : SV_Target0;
	
	void computeA()
	{
		streams.outStreamA = 2;
	}
}

shader ShaderB
{
	stage stream float4 outStreamB : SV_Target0;

	void computeB()
	{
		streams.outStreamB = 2;
	}
}

shader ShaderMain: ShaderA, ShaderB
{
	void VSMain()
	{
	
	}
	
	void PSMain()
	{
		computeA();
		computeB();
	}
}
