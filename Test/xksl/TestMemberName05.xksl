
shader ShaderMain<MemberName TStream>
{
	stage stream float4 aStream1: ASTREAM1;
	stage stream float4 aStream2: ASTREAM2;
	stage stream float4 outputStream: OUTPUTSTREAM;
	
	float4 aCol;
	
    void Compute()
	{
		streams.TStream = aCol;
	}
	
	stage void VSMain() 
    {
		Compute();
		streams.outputStream = streams.TStream;
    }
}
