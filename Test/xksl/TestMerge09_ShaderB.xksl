//Merge ShaderB into ShaderA bytecode. Expected result: 6

shader Base
{
	stream int ColorTarget: SV_Target0;

    int Compute()
    {
        return 1;
    }
    void main()
    {
        streams.ColorTarget = Compute();
    }
}

shader ShaderA : Base
{
    override int Compute()
    {
        return base.Compute() + 2;
    }
}

shader ShaderB : ShaderA
{
    override int Compute()
    {
        return base.Compute() + 3;
    }
}