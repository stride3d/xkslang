//Test "importing" a static lib when adding a composition

convertAndLoad "TestCompose16.xksl"

mixer A
A.mixin( ShaderA )

mixer B
B.mixin( CompA )

A.addComposition( ShaderA.comp = B )
A.mixin( ShaderB )
B.mixin( CompB )
A.addComposition( ShaderB.comp = B )

A.setStageEntryPoint( Pixel="main" )
A.compile
