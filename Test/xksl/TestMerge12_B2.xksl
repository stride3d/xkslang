shader Base
{
	stream int ColorTarget;

    int Compute()
    {
        return 1;
    }
	
    void main()
    {
        streams.ColorTarget = Compute();
    }
}

shader A2 : Base
{
    override int Compute()
    {
        return base.Compute() + 20;
    }
}

shader B2 : A2
{
    override int Compute()
    {
        return base.Compute() + 30;
    }
}