//Test setting compositions directly when mixin a shader

convertAndLoad "TestCompose19.xksl"

mixer B
B.mixin( ShaderA[colorA = mixin(ShaderComp), colorB = mixin(ShaderComp)] )
B.mixin( ShaderB[colorA = mixin(ShaderComp), colorB = mixin(ShaderComp)] )

B.setStageEntryPoint( Vertex="VSMain", Pixel="PSMain" )
B.compile
