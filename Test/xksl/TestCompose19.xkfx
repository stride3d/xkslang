//Test setting compositions directly when mixin a shader

convertAndLoad "TestCompose19.xksl"

mixer B
B.mixin( ShaderA[colorA1 = mixin(ShaderComp), colorA2 = mixin(ShaderComp)] )
B.mixin( ShaderB[colorB1 = mixin(ShaderComp), colorB2 = mixin(ShaderComp)] )

B.setStageEntryPoint( Vertex="VSMain", Pixel="PSMain" )
B.compile
