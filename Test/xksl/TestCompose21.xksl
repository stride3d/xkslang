shader ShaderComp
{
	float varC;
	
	float Compute()
	{
		return varC;
	}
}

shader ShaderA
{
	compose ShaderComp colorA;
	
	float Compute(){return 1 + colorA.Compute();}
}

shader ShaderB
{
	compose ShaderComp colorB;
	
	float Compute(){return 2 + colorB.Compute();}
}

shader ShaderMain : ShaderA, ShaderB
{
	compose ShaderComp colorM1;
	compose ShaderComp colorM2;
	
	float Compute(){
		return colorM1.Compute() + colorM2.Compute() + ShaderA.Compute() + ShaderB.Compute();
	}
	
	void PSMain()
	{
		float f = Compute();
	}
	
	void VSMain()
	{
		float f = Compute();
	}
}