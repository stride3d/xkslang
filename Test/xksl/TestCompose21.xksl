shader ShaderComp
{
	float varC;
	
	float Compute()
	{
		return varC;
	}
}

shader ShaderA
{
	compose ShaderComp colorA;
	
	float Compute(){return 1 + colorA.Compute();}
}

shader ShaderB
{
	compose ShaderComp colorA;
	
	float Compute(){return 2 + colorA.Compute();}
}

shader ShaderMain : ShaderA, ShaderB
{
	compose ShaderComp colorA;
	compose ShaderComp colorA0;
	
	float Compute(){
		return colorA.Compute() + colorA0.Compute() + ShaderA.Compute() + ShaderB.Compute();
	}
	
	void PSMain()
	{
		float f = Compute();
	}
	
	void VSMain()
	{
		float f = Compute();
	}
}