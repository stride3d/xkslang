shader ShaderComp
{
	float varC;
	
	float Compute()
	{
		return varC;
	}
}

shader ShaderA
{
	stage stream float totoA: TOTOA;
	
	compose ShaderComp colorA;
	compose ShaderComp colorB;
	
	float Compute(){return 5;}
	
	void VSMain()
	{
		float f = Compute();
		streams.totoA = f + colorA.Compute() + colorB.Compute();
	}
}

shader ShaderB
{
	stage stream float totoA: TOTOA;
	stage stream float totoB: TOTOB;
	
	compose ShaderComp colorA;
	compose ShaderComp colorB;
	
	float Compute(){return 5;}
	
	void PSMain()
	{
		float f = Compute();
		streams.totoB =  f + colorA.Compute() + colorB.Compute() + streams.totoA;
	}
}