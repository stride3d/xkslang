shader CompA
{
	float varCA;
	
	float Compute()
	{
		return varCA;
	}
}

shader CompB: CompA
{
	float varCB;
	
	override float Compute()
	{
		return varCB + base.Compute();
	}
}

shader ShaderA
{
	compose CompA color1;
	compose CompA color2;
	
	float main()
	{
		return color1.Compute() + color2.Compute();
	}
}
