shader ShaderMain
{
	compose ShaderCompositionWithLink composition02;
	
	stage void VSMain()
	{
		float f = 0;
		f += composition02.Compute();
	}
}

shader ShaderCompositionWithLink
{
	cbuffer cbUnstageCompositionLink
	{
		[Link("linkVar11")]
		float vara1;
		float vara2;
	}
	
	stage cbuffer cbStageCompositionLink
	{
		[Link("linkVar12")]
		float varb1;
		float varb2;
	}
	
	float Compute()
	{
		return vara1 + varb1;
	}
}

