shader Base
{
	stream int ColorTarget;

    int Compute()
    {
        return 1;
    }
	
    void main()
    {
        streams.ColorTarget = Compute();
    }
}

shader A1 : Base
{
    override int Compute()
    {
        return base.Compute() + 2;
    }
}

shader B1 : A1
{
    override int Compute()
    {
        return base.Compute() + 3;
    }
}