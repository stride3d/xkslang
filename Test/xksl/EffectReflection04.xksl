//Test reflection on a simple struct type
shader ShaderMain
{
	struct StructType
	{
		float aFloat;
		int aInt;
		bool aBool;
	};
	
	stage StructType var1;
	stage StructType var2;
	
	stage stream float4 ColorTarget : SV_Target0;
	
	stage void VSMain()
	{
	
	}
	
	stage void PSMain()
	{
		streams.ColorTarget = float4(var1.aFloat, var2.aFloat, 0, 0);
	}
};