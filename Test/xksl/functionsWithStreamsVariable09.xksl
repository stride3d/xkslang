shader TestShaderBaseA
{
	stage stream float3 sBaseA: SBASEA;

	void Compute(Streams s)
	{
		s.sBaseA = 1;
	}
}

shader TestShaderBaseB: TestShaderBaseA
{
	stage stream float3 sBaseB: SBASEB;

	override void Compute(Streams s)
	{
		s.sBaseB = 2;
		base.Compute(s);
	}
}

shader TestShaderMain: TestShaderBaseB
{
	stage stream float3 sMain: SMAIN;

	override void Compute(Streams s)
	{
		s.sMain = 1;
		base.Compute(s);
	}

	void VSMain()
	{
		Compute(streams);
	}
	
	void PSMain()
	{
		Compute(streams);
	}
}
